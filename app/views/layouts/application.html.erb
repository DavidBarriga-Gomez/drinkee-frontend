<!DOCTYPE html>
<html>
  <head>
    <title>DrinkeeFrontend</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <section="navbar">
    <% if current_user %>
      <h3>Logged in as <%= current_user.first_name%> <%= current_user.last_name %></h3>
      <%= link_to "Log out", logout_path %>
    <% else %>
      <%= link_to 'Log in with Google', user_login_path %>
    <% end %>
  </section>
  <body>
    <%= yield %>
  </body>
</html>
