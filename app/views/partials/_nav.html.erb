<section="navbar">
  <% if current_user %>
    <h3>Logged in as <%= current_user.first_name%> <%= current_user.last_name %></h3>
    <%= link_to "Dashboard", '/dashboard' %>
    <%= link_to "Cabinet", '/cabinet' %>
    <%= link_to "Ingredients", '/ingredients' %>
